	par(fig= mapzone, new=TRUE)#
	par(bg="#e9eab0")#
	par(oma= c(0,0,0,0))#
	par(mar= c(0,0,1,0))#
	barplot(plotdata, horiz=TRUE, axes=FALSE, axisnames=FALSE, col="#e9eab0", main=plottitle, cex.main=0.8)#
	important <- numeric(length=13)#
	important[11] <- christchurch[11]#
	barplot(important, horiz=TRUE, add=TRUE, col="#550000",  border="#222222", axes=FALSE, axisnames=FALSE)#
	box(which="figure", col="#222222")#
}#
#
#anticlockwise from text#
#
subplot(c(0,0.2,0.2,0.3), christchurch, "Greymouth")#
subplot(c(0,0.2,0.1,0.2), christchurch, "Invercargill")#
